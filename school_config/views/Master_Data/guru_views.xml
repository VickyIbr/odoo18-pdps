<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Master Guru List View -->
    <record id="view_master_guru_list" model="ir.ui.view">
        <field name="name">school_config.master.guru.list</field>
        <field name="model">school_config.master.guru</field>
        <field name="arch" type="xml">
            <list string="Guru">
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- Master Guru Form View -->
    <record id="view_master_guru_form" model="ir.ui.view">
        <field name="name">school_config.master.guru.form</field>
        <field name="model">school_config.master.guru</field>
        <field name="arch" type="xml">
            <form string="Guru">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Master Guru Action -->
    <record id="action_master_guru" model="ir.actions.act_window">
        <field name="name">Master Guru</field>
        <field name="res_model">school_config.master.guru</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>